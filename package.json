{
  "name": "openusercss.org",
  "version": "1.9.1",
  "release": "beta",
  "description": "OpenUserCSS",
  "main": "index.js",
  "scripts": {
    "env:dev": "env-cmd --no-override --fallback ./.env.local",
    "env:prod": "env-cmd --no-override --fallback ./.prod.env.local",
    "concurrent": "concurrently --raw --kill-others -s first",
    "ns": "npm rebuild node-sass",
    "nd": "yarn env:dev node-dev",
    "clean": "rm -rf build .nuxt dist",
    "contrib:add": "all-contributors add",
    "contrib:generate": "all-contributors generate",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "postmerge": "yarn --silent --frozen-lockfile --non-interactive --production=false",
    "postcommit": "yarn --silent --frozen-lockfile --non-interactive --production=false",
    "commit": "git-cz",
    "c": "yarn commit",
    "test": "rimraf screenshots/* && nyc ava --serial",
    "test:full": "bash ./start.sh -m -r && yarn test; bash ./start.sh -c",
    "t": "yarn test",
    "shellcheck": "node_modules/shellcheck/shellcheck-latest/shellcheck -a",
    "lint-js:dry": "eslint --format=pretty --cache --ext .js,.vue --ignore-path .gitignore src tasks test app",
    "lint-js:fix": "eslint --format=pretty --cache --ext .js,.vue --fix --ignore-path .gitignore src tasks test app",
    "lint-sh": "yarn shellcheck *.sh ci/**/*.sh",
    "lint:fix": "yarn lint-sh && lint-js:fix",
    "lint:dry": "yarn lint-sh && lint-js:dry",
    "prebuild": "yarn ns && yarn clean",
    "prerelease": "./start.sh -m && standard-version && ./start.sh -c && yarn prod",
    "release": "TAG=decentm/openusercss DOMAIN=openusercss.org yarn env:prod docker-compose -f prod.stack.yml build",
    "postrelease": "git push --follow-tags origin master && docker push decentm/openusercss",
    "prestage": "yarn --frozen-lockfile && yarn ns && yarn prod",
    "stage": "TAG=decentm/openusercss-unstable DOMAIN=staging.openusercss.org yarn env:prod docker-compose -f prod.stack.yml build",
    "poststage": "docker push decentm/openusercss-unstable",
    "wp:dev": "yarn env:dev webpack --watch --progress --run-dev",
    "wp:prod": "yarn env:prod webpack --progress --run-prod",
    "api:dev": "yarn wp:dev --config ./webpack.api.config.js",
    "api:prod": "yarn wp:prod --config ./webpack.api.config.js",
    "client:dev": "yarn wp:dev --config ./webpack.client.config.js",
    "client:prod": "yarn wp:prod --config ./webpack.client.config.js",
    "prod": "yarn env:prod gulp build && nuxt build && yarn api:prod && yarn client:prod",
    "prewatch": "yarn env:dev gulp api:fast",
    "watch": "yarn dev"
  },
  "author": "DecentM",
  "license": "GPL-3.0",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/vue"
    ]
  },
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ]
  },
